<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="300.0" prefWidth="320.0" styleClass="AnchorPane" stylesheets="@style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.fltoshi.insurancecompanyclient.controller.LoginController">
   <children>
      <TextField fx:id="loginField" layoutX="99.0" layoutY="166.0" prefHeight="26.0" prefWidth="181.0" promptText="SuperVasya" />
      <Text fontSmoothingType="LCD" layoutX="50.0" layoutY="184.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="Text" text="Логин" />
      <ButtonBar layoutX="46.0" layoutY="260.0" prefHeight="40.0" prefWidth="226.0">
         <buttons>
            <Button fx:id="enterButton" mnemonicParsing="false" onAction="#enterAction" prefHeight="28.0" prefWidth="94.0" styleClass="Button" text="Вход" />
            <Button fx:id="AddButton" mnemonicParsing="false" onAction="#registrationAction" prefHeight="28.0" prefWidth="94.0" styleClass="Button" text="Регистрация" />
         </buttons>
      </ButtonBar>
      <Text fontSmoothingType="LCD" layoutX="46.0" layoutY="231.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="Text" text="Пароль" />
      <TextField fx:id="passwordField" layoutX="99.0" layoutY="213.0" prefHeight="26.0" prefWidth="181.0" promptText="qwerty" />
      <ImageView fitHeight="127.0" fitWidth="140.0" layoutX="97.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/userIcon.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
