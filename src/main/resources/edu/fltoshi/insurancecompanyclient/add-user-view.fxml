<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="180.0" prefWidth="340.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.fltoshi.insurancecompanyclient.controller.AddUserController">
   <children>
      <TextField fx:id="userNameField" layoutX="153.0" layoutY="45.0" prefHeight="25.0" prefWidth="172.0" />
      <Text fontSmoothingType="LCD" layoutX="24.0" layoutY="62.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Имя пользователя" />
      <Text fontSmoothingType="LCD" layoutX="24.0" layoutY="109.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Пароль" />
      <TextField fx:id="passwordField" layoutX="153.0" layoutY="92.0" prefHeight="25.0" prefWidth="172.0" />
      <ButtonBar layoutX="53.0" layoutY="126.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button fx:id="saveUser" mnemonicParsing="false" onAction="#addAction" text="Сохранить" />
            <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancelAction" text="Отмена" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
